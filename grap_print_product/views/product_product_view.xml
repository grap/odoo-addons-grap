<openerp><data>

    <record id="view_product_product_form" model="ir.ui.view">
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view" />
        <field name="arch" type="xml">
            <field name="ean13" position="after">
                <button name="generate_custom_ean13" type="object" colspan="2" string="Generate Custom EAN13" groups="grap_print_product.group_generate_custom_ean13" attrs="{'invisible': [('ean13', '!=', False)]}"/>
                <button name="generate_weight_ean13" type="object" colspan="2" string="Generate Weight EAN13" groups="grap_print_product.group_generate_weight_ean13" attrs="{'invisible': [('ean13', '!=', False)]}"/>
            </field>
        </field>
    </record>

</data></openerp>
