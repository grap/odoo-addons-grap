        <field name="model">product.template</field>
        <field name="value" eval="'I0\n.'"/>
    </record>

    <record id="value_product_template_produce_delay" model="ir.values">
        <field name="name">produce_delay</field>
        <field name="key">default</field>
        <field name="key2"/>
        <field name="model">product.template</field>
        <field name="value" eval="'I0\n.'"/>
    </record>

</data></openerp>
