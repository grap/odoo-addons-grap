                                        <t t-esc="line.product_id.pricetag_organic_text"/><br/>
                                    </t>
                                </div>
                            </div><!--
                            <t t-if="line.product_id.pricetag_display_spider_chart">
                                <div class="product_price_per_uom_price">
                                    <div class="product_price">
                                        <t t-if="line.product_id.pricetag_price_volume"><b><t t-esc="line.product_id.pricetag_price_volume"/> €</b><span class="product_uom">/L</span><br/></t>
                                        <t t-if="line.product_id.pricetag_price_weight_net"><b><t t-esc="line.product_id.pricetag_price_weight_net"/> €</b><span class="product_uom">/kg</span><br/></t>
                                    </div>
                                </div>
                                <div class="product_image">
                                    <img class="product_image" t-attf-src="data:image/*;base64,{{line.product_id.spider_chart_image}}"/>
                                </div>
                            </t>-->
