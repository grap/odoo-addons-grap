<openerp><data>

    <template id="qweb_template_label_normal">
        <style type="text/css">
            body{
                font-family: arial, verdana, sans-serif;
            }
            div.product {
                border: 1px solid grey; 
                background-color:white;
                width:9.3cm; height:3.8cm;
                margin : 0 -0.2cm -0.1cm 0;
                padding : 5px;
            }
            div.product:last-child {
                padding-right: 0.08cm;
                position: relative;
                top: 2.3px;
            }
            div.floating_box {
                display : inline-block;
                vertical-align: top;
            }
            div.label_left {
                width : 6cm;
            }
            div.label_right {
                width : 3cm;
            }
            div.product_informations {
                text-align:left; font-size:8px;
                overflow : hidden;
            }
            div.product_name{
                font-size:15px; font-weight:bold;
                height: 1cm;
                padding-bottom : 5px;
                padding-top : 5px;
                overflow: hidden;
            }
            div.product_price {
                font-size: 26px; font-weight: bold;
                height: 1cm; line-height: 1cm;
                text-align: center;
                overflow: hidden;
            }
            div.product_price_per_uom_qty, div.product_price_per_uom_price {
                width : 45%;
                text-align : center;
                font-size:9px;
                height : 0.8cm;
                overflow : hidden;
                word-wrap : normal;
            }            
            div.label_bottom_right {
                margin-top:0.2cm;
                text-align:center; font-size:7px;
                overflow:hidden;
                font-family:monospace;
                position: relative; top: 6px;
            }
            div.product_labels, div.organic_text{
                height:1.0cm;
                overflow: hidden; float:left;
                text-align:left;
                padding-top: 2px;
            }            
            div.product_labels{
                padding-left : 2px;
                font-size:9px;
            }
            img.product_label{
                width:0.6cm; height:0.6cm; margin: 0.1cm 0.1cm 0.1cm -0.1cm;
            }
            div.organic_text{
                width:4cm;
                position: relative; top: 0.17cm;
                font-size: 7px;
            }
            div.wrap_ean13_image{
                overflow: hidden;
                position: relative;
                height: 1.1cm;
                bottom: 21px;
                right: 5px;
            }
            img.ean13_image{
                /* NOT YET SUPPORTED BY wkhtmltopdf clip-path: polygon(10% 20%, 90% 20%, 90% 40%, 0% 40%); */
                width:6.7cm;
                position: relative;
                top: 0.5cm;
            }
            img.product_image{
                width: 2.3cm; height: 2.3cm;
                position: relative; bottom: 0.17cm; left: 0.25cm;
            }
        </style>
        <t t-foreach="category_data['lines']" t-as="line">
            <t t-foreach="line.quantity" t-as="q">
                <div class="product floating_box">
                    <div class="label_left floating_box">
                        <div class="product_name" t-attf-style="background-color: {{ line.product_id.pricetag_type_id.color }};">
                            <t t-esc="line.product_id.name"/>
                        </div>
                        <div class="product_informations" t-attf-style="background-color: {{ line.product_id.pricetag_type_id.color }};">
                            <t t-if="line.product_id.ean13"><b><t t-esc="line.product_id.ean13"/></b><br/></t>
                            <t t-if=" not line.product_id.ean13">
                                <t t-if="line.product_id.code"><b><t t-esc="line.product_id.code"/></b><br/></t>
                            </t>
                            <t t-if="line.product_id.pricetag_origin">Origine : <b><t t-esc="line.product_id.pricetag_origin"/></b><br/></t>
                            <t t-if="not line.product_id.pricetag_origin"><br/></t>
                            <t t-if="line.product_id.maker_description">Producteur : <b><t t-esc="line.product_id.maker_description"/></b><br/></t>
                            <t t-if="not line.product_id.maker_description"><br/></t>
                        </div>
                        <div class="product_labels">
                            <!-- Product Label -->
                            <t t-foreach="line.product_id.label_ids" t-as="label">
                                <img class="product_label" t-attf-src="data:image/*;base64,{{label.image}}" t-att-alt="label.name"/>
                            </t>
                        </div>
                        <div class="organic_text">
                            <!-- Organic warning, if any -->
                            <t t-if="not line.product_id.pricetag_organic_text">
                                <t t-esc="line.product_id.pricetag_organic_text"/><br/>
                            </t>
                        </div>
                        <!-- EAN 13 Image -->
                        <div>
                            <div class="wrap_ean13_image floating_box">
                                <t t-if="line.product_id.ean13">
                                    <img class="ean13_image" t-attf-src="data:image/*;base64,{{line.product_id.ean13_image}}"/>
                                </t>
                            </div>
                        </div>
                    </div>
                    <div class="label_right floating_box">
                        <div class="product_price">
                            <t t-esc="'%0.2f' % (line.product_id.list_price)"/> €
                        </div>
                        <div class="product_price_per_uom_qty floating_box">
                            <t t-if="line.product_id.volume">Volume<br/><b><t t-esc="line.product_id.volume"/> L</b><br/></t>
                            <t t-if="line.product_id.weight_net">Poids net<br/><b><t t-esc="line.product_id.weight_net"/> kg</b><br/></t>
                        </div>
                        <div class="product_price_per_uom_price floating_box">
                            <t t-if="line.product_id.pricetag_is_second_price">
                                <t t-esc="line.product_id.pricetag_second_price_uom_text"/><br/>
                                <b><t t-esc="'%0.2f' % (line.product_id.pricetag_second_price)"/> €</b>
                            </t>
                        </div>
                        <div class="product_image">
                            <t t-if="line.product_id.pricetag_display_spider_chart">
                                <img class="product_image" t-attf-src="data:image/*;base64,{{line.product_id.spider_chart_image}}"/>
                            </t>
                        </div>
                    </div>
                </div>
            </t>
        </t>
    </template>

